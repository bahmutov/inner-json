{
  "name": "inner-json",
  "version": "0.0.0-semantic-release",
  "description": "Removes text before and after JSON object in a given file",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/bahmutov/inner-json/issues",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm prune",
        "npm run deps",
        "npm test",
        "npm run e2e",
        "npm run ban"
      ],
      "pre-push": [
        "npm run secure",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "inner-json": "src/index.js"
  },
  "files": [
    "src/*.js",
    "!src/*-spec.js"
  ],
  "homepage": "https://github.com/bahmutov/inner-json#readme",
  "keywords": [
    "clean",
    "json",
    "remove",
    "utility"
  ],
  "license": "MIT",
  "main": "src/",
  "noScopeName": "inner-json",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/inner-json.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok",
    "format": "standard-format -w src/*.js",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "standard --verbose src/*.js test/*.js",
    "pretest": "npm run format && npm run lint",
    "secure": "nsp check",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run unit",
    "unit": "mocha test/*-spec.js",
    "e2e": "node src/index.js test/example.json",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.8.3",
    "deps-ok": "1.1.0",
    "git-issues": "1.2.0",
    "license-checker": "5.1.2",
    "mocha": "2.5.3",
    "nsp": "2.5.0",
    "pre-git": "3.10.0",
    "standard": "7.1.2",
    "standard-format": "2.2.2",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "check-more-types": "2.21.0",
    "lazy-ass": "1.4.0"
  }
}
